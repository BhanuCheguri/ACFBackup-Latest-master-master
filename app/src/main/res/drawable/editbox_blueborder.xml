<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <!-- solid background -->
            <solid android:color="#ffffff"/>
            <!-- border-->
            <stroke
                android:color="@color/colorDarkBlue"
                android:width="1dp"
                />
